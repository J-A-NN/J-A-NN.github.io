.sudoku-grid {
    border-collapse: collapse; /* Essential for borders to merge into single lines */
    border: 3px solid black;   /* Thick outer boundary for the whole grid */
    margin: 20px auto;         /* Center the table on the page */
}

.sudoku-grid td {
    /* Set cell dimensions large enough for the select box */
    width: 60px;
    height: 45px;
    
    /* Default thin inner border */
    border: 1px solid #bbb;
    
    text-align: center;
    padding: 0; /* Remove default cell padding */
}

/* Style the dropdown list itself so it fits neatly */
.sudoku-grid select {
    width: 80px;
    height: 80px;
    border: none; /* Remove the default border from the select element */
    background-color: transparent;
    cursor: pointer;
    font-size: 35px; /* Make the emojis visible */
}

/* --- Prominent Borders using CSS pseudo-classes --- */

/* Thicker bottom border for the 3rd and 6th rows (tr:nth-child(3), tr:nth-child(6)) */
.sudoku-grid tr:nth-child(3n) td {
    border-bottom: 3px solid black;
}

/* Thicker right border for the 3rd and 6th columns (td:nth-child(3), td:nth-child(6)) */
.sudoku-grid td:nth-child(3n) {
    border-right: 3px solid black;
}

/* Fix: Ensure the *very last* row (row 9) and *very last* column (col 9) use the thin border internally, 
   as the thick outer table border handles the edge */
.sudoku-grid tr:nth-child(9n) td {
    border-bottom: 1px solid #a3a3a3;
}

.sudoku-grid td:nth-child(9n) {
