        <script>
        function entreeValides() {
            const solutions = ["â˜ƒï¸", "ğŸ", "ğŸ¥›", "ğŸ¦Œ", "ğŸ„", "ğŸª", "ğŸ…", "ğŸ…", "ğŸ„", "ğŸ¥›", "â„ï¸", "ğŸª", "ğŸŒŸ", "ğŸ¦Œ", "ğŸ¦Œ", "ğŸ„", "ğŸ", "â„ï¸", "â˜ƒï¸", "ğŸ…", "ğŸŒŸ", "ğŸª", "ğŸ¦Œ", "â„ï¸", "ğŸ", "â˜ƒï¸", "ğŸ¥›", "â„ï¸", "â˜ƒï¸", "ğŸª", "ğŸ", "ğŸ¥›", "ğŸ¦Œ", "ğŸ„", "ğŸ", "â˜ƒï¸", "ğŸ„", "ğŸ…", "â„ï¸", "â˜ƒï¸", "ğŸ„", "ğŸŒŸ", "ğŸ…", "ğŸª", "ğŸª", "ğŸ¥›", "â„ï¸", "ğŸ…", "â˜ƒï¸", "ğŸŒŸ", "ğŸ", "ğŸ", "ğŸŒŸ", "â˜ƒï¸", "ğŸ¥›", "ğŸª", "ğŸ¦Œ", "ğŸ„"];
            for (let i = 0; i < solutions.length; i++) {
            const caseId = "case" + (i + 1);
            const userChoice = document.getElementById(caseId).value;
            if (userChoice !== solutions[i]) {
                return false;
            }
            }
            return true;
        }

        function verifierSolution() {
            var affiche = document.getElementById("divAffiche");
            if (entreeValides()) {
            message = "FÃ©licitations ! Vous avez rÃ©solu le puzzle !";
            updateScore(1000)
            } else {
            message = "Certaines rÃ©ponses sont incorrectes. Veuillez rÃ©essayer.";
            updateScore(-500)
            }
            affiche.innerHTML = message;
        }
function styleSudokuBorders() {
    const table = document.getElementById('sudoku-board');
    if (!table) return;

    for (let i = 0; i < 9; i++) {
      for (let j = 0; j < 9; j++) {
        // Get the cell at row i, column j
        const cell = table.rows[i].cells[j];

        // Apply a thicker bottom border to the 3rd and 6th rows
        if ((i + 1) % 3 === 0 && i !== 8) {
          cell.style.borderBottom = '3px solid black';
        }

        // Apply a thicker right border to the 3rd and 6th columns
        if ((j + 1) % 3 === 0 && j !== 8) {
          cell.style.borderRight = '3px solid black';
        }
      }
    }
  }
  styleSudokuBorders();


let score = 0;

const scoreDisplay = document.getElementById("scoreDisplay");

function updateScore(points) {
  score += points; 
  scoreDisplay.innerHTML = score; 
}
        </script>
